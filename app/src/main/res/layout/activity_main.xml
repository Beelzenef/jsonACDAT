<LinearLayout android:layout_width="match_parent"
              android:layout_height="match_parent"
              xmlns:tools="http://schemas.android.com/tools"
              tools:context="com.example.jsonacdat.MainActivity"
              android:orientation="vertical"
              xmlns:android="http://schemas.android.com/apk/res/android">

    <Button
        android:id="@+id/btn_Primitiva"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Primitiva"/>

    <Button
        android:id="@+id/btn_Contactos"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Contactos"/>

    <Button
        android:id="@+id/btn_ContactosGSON"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Contactos GSON"/>

    <Button
        android:id="@+id/btn_Creacion"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Creacion"/>

    <Button
        android:id="@+id/btn_Retrofit"
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:onClick="onClick_lanzarExs"
        android:text="Retrofit"/>

</LinearLayout>